{
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "react/prop-types": "warn",
    "no-unused-vars": "warn"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier", "jest"]
}
